<Project ToolsVersion="15.0">
  <Import Project="$(MSBuildExtensionsPath)\$(MSBuildToolsVersion)\Microsoft.Common.props" />

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>$if$ ($context$ == WebCore)netcoreapp1.0$else$$netframeworkversion$$endif$</TargetFramework>
    <PreserveCompilationContext>true</PreserveCompilationContext>$if$ ($context$ == WebCoreFullFramework)
    <RuntimeIdentifier>win7-x86</RuntimeIdentifier>$endif$
  </PropertyGroup>$if$ ($context$ == WebCore)

  <PropertyGroup>
    <PackageTargetFallback>$(PackageTargetFallback);portable-net45+win8+wp8+wpa81;</PackageTargetFallback>
  </PropertyGroup>$endif$

  <ItemGroup>
    <Compile Include="**\*.cs" Exclude="$(GlobalExclude)" />
    <EmbeddedResource Include="**\*.resx" Exclude="$(GlobalExclude)" />
  </ItemGroup>$if$ ($context$ == WebCoreFullFramework)

  <ItemGroup>
    <Reference Include="System.ComponentModel.DataAnnotations" />
    <Reference Include="System.Data" />
  </ItemGroup>$endif$

  <ItemGroup>$if$ ($context$ == WebCore)
    <PackageReference Include="Microsoft.NETCore.App" Version="__NetCorePlatformVersion__" />$endif$
    <PackageReference Include="Microsoft.NET.Sdk.Web" Version="__WebSdkVersion__" />
    <PackageReference Include="Microsoft.AspNetCore.Diagnostics" Version="__AspNetCoreReleasePackageVersion__" />
    <PackageReference Include="Microsoft.AspNetCore.Mvc" Version="__AspNetCorePatchPackageVersion__" />
    <PackageReference Include="Microsoft.AspNetCore.Razor.Tools" Version="__ToolsPackageVersion__" />
    <PackageReference Include="Microsoft.AspNetCore.Routing" Version="__AspNetCorePatchPackageVersion__" />
    <PackageReference Include="Microsoft.AspNetCore.Server.IISIntegration" Version="__AspNetCoreReleasePackageVersion__" />
    <PackageReference Include="Microsoft.AspNetCore.Server.Kestrel" Version="__AspNetCorePatchPackageVersion__" />
    <PackageReference Include="Microsoft.AspNetCore.StaticFiles" Version="__AspNetCoreReleasePackageVersion__" />
    <PackageReference Include="Microsoft.Extensions.Configuration.EnvironmentVariables" Version="__AspNetCoreReleasePackageVersion__" />
    <PackageReference Include="Microsoft.Extensions.Configuration.Json" Version="__AspNetCoreReleasePackageVersion__" />
    <PackageReference Include="Microsoft.Extensions.Logging" Version="__AspNetCoreReleasePackageVersion__" />
    <PackageReference Include="Microsoft.Extensions.Logging.Console" Version="__AspNetCoreReleasePackageVersion__" />
    <PackageReference Include="Microsoft.Extensions.Logging.Debug" Version="__AspNetCoreReleasePackageVersion__" />
    <PackageReference Include="Microsoft.Extensions.Options.ConfigurationExtensions" Version="__AspNetCoreReleasePackageVersion__" />
    <PackageReference Include="Microsoft.VisualStudio.Web.BrowserLink.Loader" Version="14.0.0" />
  </ItemGroup>
  <ItemGroup>
    <DotNetCliToolReference Include="BundlerMinifier.Core" Version="__BundlerPackageVersion__" />
  </ItemGroup>

$packagereferenceslist$

  <Import Project="$(MSBuildToolsPath)\Microsoft.CSharp.targets" />

<!--
   <Target Name="BeforePublish">
    <Exec Command="bower install" />
    <Exec Command="dotnet bundle" />
  </Target>
-->
</Project>
